    <div class="signerscontainer">
        <div class="allsigners">
            All {{numOfSigns}} signers:
        </div>
        <form action="signersin" method="POST">
            {{#allInfo}}
               {{#if url}}
                    <div class="inline">
                       <div>
                           <a href="{{url}}" class="link"> {{first}} {{last}}</a>&nbsp;
                       </div>
                       <div class="beige">
                           age: {{age}} city:
                           <button type="submit" name="button" class="nobutton link" value="{{city}}">{{city}}</button>
                       </div>
                    </div>


               {{else}}
                   <div class="inline beige">
                       <div>
                            {{first}} {{last}} {{#if age}} age: {{age}}&nbsp;{{/if}}
                       </div>
                       {{#if city}}
                           <div>
                                 <button type="submit" name="button" class="nobutton link" value="{{city}}">{{city}}</button>
                           </div>
                       {{/if}}
                   </div>

               {{/if}}

            {{/allInfo}}
        <input type="hidden" name="_csrf" value="{{csrfToken}}">
        </form>
    </div>


